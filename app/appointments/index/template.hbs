{{#item-listing paginationProps=paginationProps }}
  <table class="table">
    <tr class="table-header">
      <th>Date</th>
      <th>Name</th>
      <th>Type</th>
      <th>Location</th>
      <th>Provider</th>
      <th>Status</th>
      <th>Actions</th>
    </tr>
    {{#each model as |appointment|}}
      {{partial 'appointments/item'}}
    {{/each}}
  </table>
{{/item-listing}}
