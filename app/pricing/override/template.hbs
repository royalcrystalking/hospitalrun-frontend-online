{{#modal-dialog
    isUpdateDisabled=isUpdateDisabled
    title=title
    updateButtonAction=updateButtonAction
    updateButtonText=updateButtonText }}
  {{#em-form model=model submitButton=false}}
    {{em-select label="Pricing Profile"
      property="profile"
      content=pricingProfiles
      optionValuePath="selectObject"
      optionLabelPath="selectObject.name"
      class="required"
      prompt=" "
    }}
    {{em-input label="Price" property="price" class="required"}}
  {{/em-form}}
{{/modal-dialog}}
