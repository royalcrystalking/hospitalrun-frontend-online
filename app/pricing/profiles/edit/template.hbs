{{#modal-dialog
    isUpdateDisabled=isUpdateDisabled
    title=title
    updateButtonAction=updateButtonAction
    updateButtonText=updateButtonText }}
  {{#em-form model=model submitButton=false as |form|}}
    {{form.input property="name" label=(t 'labels.name') class="required pricing-profile-name"}}
    {{number-input model=model property="discountPercentage" label=(t 'pricing.labels.discountPercentage') class="pricing-profile-percentage"}}
    {{number-input model=model property="discountAmount" label=(t 'pricing.labels.discountAmount') class="pricing-profile-discount"}}
    {{form.input property="setFee" label=(t 'pricing.labels.setFee') class="pricing-set-fee"}}
  {{/em-form}}
{{/modal-dialog}}
