{{#edit-panel editPanelProps=editPanelProps}}
  <div class="panel panel-primary">
    <div class="panel-body">
      {{#em-form model=model submitButton=false as |form|}}
        {{form.input label=(t 'admin.header.facilityName') property="value.facilityName"}}
        {{form.input label=(t 'admin.header.headerLine1') property="value.headerLine1"}}
        {{form.input label=(t 'admin.header.headerLine2') property="value.headerLine2"}}
        {{form.input label=(t 'admin.header.headerLine3') property="value.headerLine3"}}
        {{form.input label=(t 'admin.header.logoURL') property="value.logoURL"}}
      {{/em-form}}
    </div>
  </div>
{{/edit-panel}}
